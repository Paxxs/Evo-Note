      "backgroundColor": "default",
      "textAlignment": "left"
    },
    "content": [
      {
        "type": "text",
        "text": "1st level of puns: 5 gold coins",
        "styles": {}
      }
    ],
    "children": []
  },
  {
    "id": "72d5b241-b8d0-41cd-9af2-f10b70a87694",
    "type": "bulletListItem",
    "props": {
      "textColor": "default",
